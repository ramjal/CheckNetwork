<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  
  <appSettings>    
    <!--Ping default timeout value is 4000 milliseconds-->
    <add key="Timeout" value="2000" />
    <add key="SleepTime" value="2000" />    
    <!--Google DNS server IPv4: 8.8.8.8 and/or 8.8.4.4. For IPv6: 2001:4860:4860::8888 and/or 2001:4860:4860::8844-->
    <add key="HostNameOrAddress" value="2001:4860:4860::8888" />
    <!--<add key="HostNameOrAddress" value="8.8.8.8" />-->
    <add key="OutputFilePath" value="CheckNetwork.log" />
    <add key="ConsoleOutput" value="true" />
  </appSettings>

</configuration>